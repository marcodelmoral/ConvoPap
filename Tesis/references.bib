journal = {Biomedical Engineering {\&} Computational Biology},
keywords = {and future,artificial intelligence,artificial neural networks,artificial neural networks as,automation,biomedical engineering and computational,citation,computer-assisted,cytology,cytopathology,decision support,decision support tools in,diagnosis,ia,medicina,neural networks,past,pouliakis et al,present,redes neuronales,review},
