file = {:C$\backslash$:/Users/marco/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Meza-Palacios et al. - 2017 - Development of a fuzzy expert system for the nephropathy control assessment in patients with type 2 diabet.pdf:pdf},
issn = {09574174},
journal = {Expert Systems with Applications},
keywords = {Expertise,Fuzzy expert system,Nephropathy control,Response surface,Type 2 diabetes mellitus},
number = {1},
pages = {335--343},
publisher = {Elsevier Ltd},
title = {{Development of a fuzzy expert system for the nephropathy control assessment in patients with type 2 diabetes mellitus}},
url = {http://dx.doi.org/10.1016/j.eswa.2016.10.053},
volume = {72},
year = {2017}
}
@article{Perez-Gallardo2008,
